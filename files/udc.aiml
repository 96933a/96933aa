<?xml version="1.0" encoding="UTF-8"?>
<aiml>
  <category>
    <pattern>*</pattern>
    <template>I have no answer for that.</template>
  </category>
  
  <category>
      <pattern>HELLO</pattern>
      <template>
          <random>
             <li>Hi</li>
             <li>Hello</li>
             <li>Nice to see you</li>
          </random>
      </template>
  </category>
  
  <category>
      <pattern>MY FULL NAME is * *</pattern>
      <template>Hi <star index="1"/>. How are you today Mr <star index="2"/>?</template>
  </category>
  
  <category>
      <pattern>HOW WAS YOUR DAY TODAY</pattern>
      <template>It was great, how about ours?</template>
  </category>
  
  <category>
      <pattern>MY DAY WAS GREAT</pattern>
      <template>Thats good!</template>
  </category>
  
  <category>
      <pattern>WHAT IS YOUR FAVORITE SPORT</pattern>
      <template>Boxing, what about you?</template>
  </category>
  
  <category>
      <pattern>SAME</pattern>
      <template>Thats Fantastic!</template>
  </category>
  
  <category>
      <pattern>WHO IS YOUR FAVORITE BOXER</pattern>
      <template>
         <random>
            <li>Floyd Mayweather</li>
            <li>Tyson Fury</li>
            <li>Ryan Garcia</li>
           <li>KSI</li>
           <li>Deontay Wilder</li>
           <li>Mike Tyson</li>
           <li>Devin Haney</li>
         </random>
      </template>
  </category>
  
<category>
    <pattern>WHO DO YOU THINK IS THE MOST SKILLED BOXER EVER</pattern>
    <template>
    <random>
     <li>Floyd Mayweather</li>
            <li>Tyson Fury</li>
            <li>Wladamir Klitscko</li>
           <li>Muhammed Ali</li>
           <li>Rocky Marciano</li>
           <li>Roy Jones Junior</li>
    </random>
    </template>
</category>

